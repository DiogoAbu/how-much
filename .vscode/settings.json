{
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[xml]": {
    "editor.defaultFormatter": "DotJoshJohnson.xml"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "conventionalCommits.autoCommit": false,
  "conventionalCommits.gitmoji": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.formatOnSave": true,
  "eslint.enable": true,
  "eslint.format.enable": true,
  "i18n-ally.enabledFrameworks": ["i18next", "react"],
  "i18n-ally.keepFulfilled": true,
  "i18n-ally.keystyle": "nested",
  "i18n-ally.localesPaths": "src/services/locales",
  "i18n-ally.namespace": true,
  "i18n-ally.sortKeys": true,
  "tslint.enable": false,
  "xmlTools.enforcePrettySelfClosingTagOnFormat": true,
  "xmlTools.splitAttributesOnFormat": true
}
