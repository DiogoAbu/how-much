{
  "version": "0.2.0",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "android-local": "yarn reactotron && expo start --android --host localhost",
    "ios": "expo start --ios",
    "ios-local": "expo start --ios --host localhost",
    "web": "expo start --web",
    "eject": "expo eject",
    "doctor": "expo doctor",
    "reactotron": "adb reverse tcp:9090 tcp:9090",
    "typecheck": "tsc --noEmit",
    "test": "echo \"No test specified\" && exit 0",
    "eslint": "eslint . --ext .ts,.tsx,.js --max-warnings 0 --config ./.eslintrc",
    "prettier": "prettier \"**/*.{json,eslintrc,prettierrc}\" --config ./.prettierrc",
    "lint": "yarn eslint && yarn prettier",
    "lint-fix": "yarn eslint --fix && yarn prettier --write",
    "fix-lockfile": "node ./scripts/fix-lockfile.js",
    "postinstall": "patch-package"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && yarn fix-lockfile && yarn doctor && yarn typecheck && yarn test --forceExit --silent --onlyChanged",
      "post-commit": "git update-index --again",
      "pre-push": "git fetch --prune origin \"+refs/tags/*:refs/tags/*\""
    }
  },
  "lint-staged": {
    "*.{j,t}s{,x}": "eslint --config ./.eslintrc --cache --fix",
    "*.json": "prettier --config ./.prettierrc --write"
  },
  "dependencies": {
    "@react-native-community/async-storage": "~1.12.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/native": "^5.7.5",
    "@react-navigation/stack": "^5.9.2",
    "buffer": "^5.6.0",
    "countries-list": "^2.5.6",
    "expo": "~38.0.8",
    "expo-ads-admob": "~8.2.1",
    "expo-constants": "~9.1.1",
    "expo-localization": "~8.2.1",
    "expo-media-library": "~8.2.1",
    "expo-sharing": "~8.2.1",
    "expo-status-bar": "^1.0.2",
    "i18next": "^19.8.2",
    "intl": "^1.2.5",
    "material-color-hash": "^0.1.6",
    "mobx": "^6.0.0",
    "mobx-react-lite": "^3.0.0",
    "mobx-sync": "^3.0.0",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "react": "~16.11.0",
    "react-dom": "~16.11.0",
    "react-i18next": "^11.7.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.2.tar.gz",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-paper": "^4.2.0",
    "react-native-reanimated": "~1.9.0",
    "react-native-redash": "^15.5.2",
    "react-native-safe-area-context": "~3.0.7",
    "react-native-screens": "~2.9.0",
    "react-native-skeleton-content": "^1.0.20",
    "react-native-svg": "12.1.0",
    "react-native-uuid": "^1.4.9",
    "react-native-view-shot": "3.1.2",
    "react-native-web": "~0.11.7",
    "react-navigation-collapsible": "^5.6.4",
    "sort-array": "^4.1.2",
    "victory-native": "^35.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@types/react": "~16.9.49",
    "@types/react-native": "~0.63.23",
    "@types/react-native-uuid": "^1.4.0",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "dotenv": "^8.2.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-react-native": "^3.10.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-unused-imports": "^0.1.3",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.0",
    "prettier": "^2.1.2",
    "reactotron-react-native": "^5.0.0",
    "semantic-release": "^17.2.1",
    "semver": "^7.3.2",
    "typescript": "~4.0.3"
  }
}
