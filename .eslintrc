{
  "extends": [
    "superop",
    "superop/import-resolver-babel",
    "superop/typescript",
    "superop/react",
    "superop/react-native"
  ],
  "rules": {
    "react/jsx-curly-newline": 0,
    "react/jsx-indent": 0
  },
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {
        "@typescript-eslint/restrict-template-expressions": [2, { "allowAny": true, "allowNullish": true }]
      }
    }
  ]
}
